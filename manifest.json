{
  "manifest_version": 3,
  "name": "SweetAlert Injector",
  "version": "1.0",
  "description": "Injects SweetAlert into webpages.",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
